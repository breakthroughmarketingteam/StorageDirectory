<div id="type-one-top-bar">
	<div id="type-one-top-bar-head">
	   <ul>
		 <li id="list-name"><a href=""><span class="hidden">List by </span>Facility name</a></li>
		 <li id="list-miles"><a href=""><span class="hidden">List by </span>Miles</a></li>
	   </ul>
	</div>
		 
	<div id="rslt-list-bg">
			
	  <%= render :partial => 'listings/result', :collection => @listings %>
	  <% if @listings.blank? %>
        <div class="no_results framed">
          Sorry, that search didn't return any results. Try using less filter options.
        </div>
      <% end %>
				
	</div>
	
	<% unless @listings.blank? %>
    <div class="pagination">
      <p class="rslt-foot">
        <%= more_results_link(@listings) %>
      </p>
      <span id="per_page" class="hidden"><%= @listings.per_page if @listings.respond_to?(:per_page) %></span>
    </div>
  <% end %>
</div>
