<% declare_content_for %>
<div id="ajax_wrap_inner">
  <%= display_message flash %>

  <%= render :partial => 'shared/model_options', :locals => { :model => model, :edit_path => edit_model_path(model), :hide_anchor => 'hide_collapseable_staticContent' } %>
	
  <%= show_title model %>

  <div class="collapseable stack">
  	<%= render_model_content(model) %>
  </div>
  
  <%= render :partial => 'shared/model_new_back_buttons' %>
</div>