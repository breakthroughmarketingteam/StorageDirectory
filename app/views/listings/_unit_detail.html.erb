<div class="unit_detail_in">
	<div class="left">
		<% if listing.web_special %>
			<%= listing.web_special.label %>: <span title="<%= listing.web_special.description %>"><%= listing.web_special.title %></span>
		<% end %>
	</div>
	
	<div class="compare">
		<%= label_tag "compare_#{listing.id}", 'Compare 2 or more' %><input type="checkbox" value="<%= listing.id %>" name="compare" id="compare_<%= listing.id %>">	
	</div>
</div>